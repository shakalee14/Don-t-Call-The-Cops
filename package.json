    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "body-parser": "~1.15.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "chai-jsx": "^1.0.1",
    "cheerio": "^0.22.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "enzyme": "^2.6.0",
    "express": "~4.13.4",
